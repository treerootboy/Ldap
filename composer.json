{
    "name": "xavrsl/ldap",
    "description": "Add Ldap Search possibilities to Laravel",
    "keywords": ["laravel", "ldap", "search"],
	"license": "MIT",
    "authors": [
        {
            "name": "Xavier Roussel",
            "email": "contact@xavrsl.fr"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-ldap": "*",
        "illuminate/support": "~5.0"
    },
    "autoload": {
        "psr-0": {
            "Xavrsl\\Ldap": "src/"
        }
    },
    "extra": {
		"laravel": {
			"providers": [
				"Xavrsl\\Ldap\\LdapServiceProvider"
			],
			"aliases": {
				"Ldap": "Xavrsl\\Ldap\\Facades\\Ldap"
			}
		}	    
    }
}
